     TA_t0       = 0.0022    , # mol/kg-soln  (comparable to Millero1998)

     kc          = 0.5       , # 1/d	         proportionality factor for air-water exchange
     kp          = 0.000001  , # mol/(kg-soln*d)	 max rate of calcium carbonate precipitation
